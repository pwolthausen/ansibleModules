Include ~/.ssh/conf.d/*

Host *
   GSSAPIAuthentication no
   ServerAliveInterval 60
   ServerAliveCountMax 15
   IdentityFile ~/.ssh/id_rsa
   IdentitiesOnly yes
   StrictHostKeyChecking no
   user {{ user }}


Host bastion
   hostname map.cloudops.net
   IdentityFile ~/.ssh/id_rsa
   user {{ user }}
